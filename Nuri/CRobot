public class CRobot {

    private	Maze maze;
    private PathFinder path;
    public CRobot(Maze maze) {
        this.maze=maze;
    }

    public void moveC() {

        findpath();
        while(true){

        }
    }
    public void findpath(){
        path.generatePath();
        path.markpath(path.getcurrent());
        path.MazeWithPath();
    }

}
